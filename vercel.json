{
  "functions": {
    "api/cron/daily-sync.js": {
      "maxDuration": 300
    },
    "api/supabase/analytics.js": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cron/daily-sync",
      "schedule": "0 8 * * *"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install && cd client && npm install"
}